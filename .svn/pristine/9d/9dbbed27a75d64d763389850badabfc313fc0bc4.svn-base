<body id="pagebody">
    <div class="wapper">
        @{ var mod = Request["url"];
            var clshome = "";
            if (mod == "/")
            {
                clshome = "bordernone";
            }
        }
        @{ Html.RenderAction("HomeBanner", "Home"); }
        @{Html.RenderAction("HomeMenu", "MainMenu");}
        <section id="body-content">
            <div class="container">
                <div class="row">
                    @{ Html.RenderAction("PathWay", "Home"); }
                    @RenderBody()
                </div>
            </div>
        </section>
        @{ Html.RenderAction("HomeFooter", "Home"); }
    </div>
