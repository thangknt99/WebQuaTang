@model  List<tbl_Footer>
@using Web.Model
@using Web
@using Web.Areas.Admin.Controllers

@{
    Layout = null;
}
<style>
    .to-top {
        z-index: 999;
        position: fixed;
        bottom: 10px;
        right: 10px;
    }
</style>
<a href="#top" class="btn btn-default to-top" style="display:none;"><i class="fa fa-arrow-up"></i></a>
<footer id="footer">
    <div class="container">
        <div class="row">
            <div class="body-footer">
                <div class="footer" style="background: #055296"> </div>
                @foreach (var item in Model)
                {
                    <div style="text-transform: uppercase;text-align:center;font-weight:bold;margin-top:20px">@item.Name</div><br/>
                    <div style="margin-top:-5px;text-align:center">@item.Address</div><br />
                    <div style="margin:-15px 0px 40px 0px;text-align:center">Email: @item.Email- Điện thoại:@item.Phone</div> 
                }
                 
            </div>
        </div>
    </div>
</footer>
<style>
    .navbar-default{background: @ConfigTextController.GetValueCFT("mau_menu") !important}
    #dosinot .nav-pills > li > a, .boxhomevz .title_panel a, .navbar-collapse ul li a:hover, .navbar-collapse ul li li a:hover, .navbar-collapse ul li a.active {background: @ConfigTextController.GetValueCFT("mau_tab_active") !important;}
    .boxhomevz .title_panel, #dosinot .nav-pills > li, .homevbz ul {border-bottom: 4px solid @ConfigTextController.GetValueCFT("mau_tab_active") !important;}
    .homevbz li.active {background: @ConfigTextController.GetValueCFT("mau_tab_active") !important;}
    .homevbz li{background: @ConfigTextController.GetValueCFT("mau_tab_inactive") !important;}
    .footer {border-top: 10px solid @ConfigTextController.GetValueCFT("mau_borderfooter") !important;}
    .navbar-nav > li.dropdown:hover {background: @ConfigTextController.GetValueCFT("mau_menu_hover") !important;}
</style> 