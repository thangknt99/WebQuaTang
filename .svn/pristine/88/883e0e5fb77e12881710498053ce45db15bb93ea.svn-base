@{
    ViewBag.Title = "HomeLinkWebSite";
    Layout = null;
    var lstLinkWebSite = (List < tbl_LinkWebsite >) TempData["lstLinkWebSite"];
}
@using Web.Areas.Admin.Controllers
@using Web.Core
@using Web.Model
@model Web.Model.tbl_LinkWebsite
<div class="col-md-12 box_right">
    <div class="row">
        <div id="dosinot">
            <ul class="nav nav-pills" style="background:#055296;">
                <li class="active" style="border-left:none;border-right:none;padding-left:2px;">
                    <a href="#">@ConfigTextController.GetValueCFT("LienKetLinkWebSite")</a>
                </li>
            </ul>
            <div class="tab-content clearfix" id="link-website">
                @Html.DropDownListFor(g=>g.Name, new SelectList(lstLinkWebSite, "Url", "Name"), "--Chọn liên kết--", new { @class = "form-control ", @onchange = "window.open(this.options[this.selectedIndex].value,'_blank')" })
            </div>
        </div>
    </div>
</div>

